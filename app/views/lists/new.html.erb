<div class="container">

  <h1>Lists#new</h1>
  <p>Find me in app/views/lists/new.html.erb</p>
  <h1>New List</h1>


  <form>
    <%= form_with model: @list, url: lists_path do |form| %>
      <div>
        <%= form.label :name %>
        <%= form.text_field :name %>
      </div>

      <%= form.submit %>
    <% end %>
  </form>

  <%= link_to 'Back', lists_path, class: "btn btn-gradient" %>


</div>
